{
    "name": "Extension",
    "version": "0.1.0",
    "description": "Hackathon Team X",
    "permissions": ["storage", "activeTab","tabs"],
    "background": {
        "service_worker": "background.js",
        "type": "module"
      },
      "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
    "action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 3
}